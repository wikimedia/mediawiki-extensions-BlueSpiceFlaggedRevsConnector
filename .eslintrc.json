{
	"root": true,
	"extends": [
		"eslint:recommended",
		"wikimedia/mediawiki",
		"wikimedia/jquery"
	],
	"globals": {
		"Ext": "readonly",
		"jQuery": "readonly",
		"BS": "readonly",
		"bs": "readonly",
		"mediaWiki": "readonly",
		"blueSpice": "readonly"
	},
	"rules": {
		"no-console": "error",
		"no-alert": "error",
		"no-unused-vars": "off"
	},
	"env": {
		"browser": true,
		"es6": true
	}
}
